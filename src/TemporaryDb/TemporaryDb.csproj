<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Nullable>enable</Nullable>
    <LangVersion>8.0</LangVersion>
    <PackageId>TemporaryDb</PackageId>
    <Version>2.0.0</Version>
    <Authors>Josh Clark</Authors>
    <Description>Library to create a temporary database that can be used for testing and then dropped once the tests are complete</Description>
    <RepositoryUrl>https://github.com/joshclark/TemporaryDb</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright Â© Josh Clark 2024</Copyright>
    <PackageTags>Temporary Db;LocalDB</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.1.4" />
  </ItemGroup>
</Project>